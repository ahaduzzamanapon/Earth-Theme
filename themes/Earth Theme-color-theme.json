	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5e98b3"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#89DDFF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Rainbow Brackets 1",
			"scope": "punctuation.section.bracket",
			"settings": {
				"foreground": "#FF5370" // Red color for brackets
			}
		},
		{
			"name": "Rainbow Brackets 2",
			"scope": "punctuation.section.brace",
			"settings": {
				"foreground": "#82AAFF" // Blue color for braces
			}
		},
		{
			"name": "Rainbow Brackets 3",
			"scope": "punctuation.section.parenthesis",
			"settings": {
				"foreground": "#C3E88D" // Green color for parentheses
			}
		},
		{
			"name": "Rainbow Brackets 4",
			"scope": "punctuation.section.angle",
			"settings": {
				"foreground": "#F78C6C" // Orange color for angle brackets
			}
		},
		{
			"name": "Rainbow Brackets 5",
			"scope": "punctuation.section.embedded",
			"settings": {
				"foreground": "#B2CCD6" // Cyan color for embedded brackets
			}
		},
		{
			"name": "Rainbow Brackets 6",
			"scope": "punctuation.section.array",
			"settings": {
				"foreground": "#C792EA" // Purple color for array brackets
			}
		},
		{
			"name": "Rainbow Brackets 7",
			"scope": "punctuation.section.parameters",
			"settings": {
				"foreground": "#89DDFF" // Light blue color for parameter brackets
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
